{
  "name": "opencode-openai-proxy",
  "version": "0.1.0",
  "description": "OpenAI-compatible proxy for OpenCode OAuth credentials",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "test": "SKIP_E2E=1 bun test",
    "test:unit": "SKIP_E2E=1 bun test tests/converters",
    "test:e2e": "bun test tests/e2e",
    "test:e2e:stream": "bun run scripts/e2e-stream.ts",
    "test:agent": "bun run tests/agent-loop.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.57",
    "@ai-sdk/google": "2.0.52",
    "@ai-sdk/google-vertex": "3.0.97",
    "@ai-sdk/openai": "2.0.89",
    "@ai-sdk/openai-compatible": "1.0.31",
    "@tarquinen/opencode-auth-provider": "file:tarquinen-opencode-auth-provider-0.1.8.tgz",
    "ai": "5.0.123",
    "hono": "4.11.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "openai": "6.16.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
